{
    "env": {
        "browser":true,
        "es2021":true
    
    },
    "extends":[
        "eslint:recommanded",
        "plugin: react/recommanded",
        "plugin:react-hooks/recommanded"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["react, react-hooks"],
    "rules": {
        "react/react-in-jsx-scope":0,
        "react/prop-types":0,
        "react/no-unescaped-entities":0,
        "react/hooks/exhaustive-deps":1,
        "react/hooks/rules-of-hooks":"error"
    }
}